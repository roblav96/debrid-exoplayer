{
	"name": "debrid-exoplayer",
	"version": "0.0.0",
	"main": "dist/main.js",
	"scripts": {
		"start": "node --require ts-node/register/transpile-only src/main.ts",
		"dev": "cross-env NODE_ENV=development node --inspect --require node-env-dev --require ts-node/register/transpile-only src/main.ts",
		"watch": "cross-env NODE_ENV=development nodemon --quiet --no-stdin --watch src --ext ts --exec 'node --inspect --require node-env-dev --require ts-node/register/transpile-only' src/main.ts"
	},
	"dependencies": {
		"appium-adb": "^8.7.2",
		"get-stdin": "^8.0.0",
		"mri": "^1.1.6",
		"rambda": "^6.1.0",
		"rambdax": "^6.1.0",
		"string-fn": "^2.12.2"
	},
	"devDependencies": {
		"@types/mri": "^1.1.0",
		"@types/node": "^14.11.2",
		"cross-env": "^7.0.2",
		"dts-generate": "^1.4.5",
		"node-env-dev": "^1.9.0",
		"nodemon": "^2.0.4",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3"
	},
	"license": "Unlicense",
	"author": "Robert Laverty <roblav96@gmail.com> (https://github.com/roblav96)",
	"homepage": "https://github.com/roblav96/debrid-exoplayer#readme",
	"repository": "github:roblav96/debrid-exoplayer",
	"bugs": "https://github.com/roblav96/debrid-exoplayer/issues"
}
